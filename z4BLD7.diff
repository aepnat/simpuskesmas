+        //----------------------------------------------------------------------
+        public static function maximumWords($mode, $version)
+        {
+            if ($mode == QR_MODE_STRUCTURE) {
+                return 3;
+            }
+
+            if ($version <= 9) {
+                $l = 0;
+            } elseif ($version <= 26) {
+                $l = 1;
+            } else {
+                $l = 2;
+            }
+
+            $bits = self::$lengthTableBits[$mode][$l];
+            $words = (1 << $bits) - 1;
