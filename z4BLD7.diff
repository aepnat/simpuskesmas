+        /**
+         * makeMaskNo.
+         *
+         * @param int $maskNo
+         * @param int $width
+         * @param int $s
+         * @param int $d
+         * @param bool $maskGenOnly
+         *
+         * @return int b
+         */
+        protected function makeMaskNo($maskNo, $width, $s, &$d, $maskGenOnly = false)
+        {
+            $b = 0;
+            $bitMask = [];
+            $bitMask = $this->generateMaskNo($maskNo, $width, $s, $d);
+            if ($maskGenOnly) {
+                return;
+            }
+            $d = $s;
+            for ($y = 0; $y < $width; $y++) {
+                for ($x = 0; $x < $width; $x++) {
+                    if ($bitMask[$y][$x] == 1) {
+                        $d[$y][$x] = chr(ord($s[$y][$x]) ^ (int) $bitMask[$y][$x]);
+                    }
+                    $b += (int) (ord($d[$y][$x]) & 1);
+                }
+            }
+
+            return $b;
+        }
