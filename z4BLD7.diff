+            for ($x = 0; $x < $width; $x++) {
+                $head = 0;
+                $this->runLength[0] = 1;
+
+                for ($y = 0; $y < $width; $y++) {
+                    if ($y == 0 && (ord($frame[$y][$x]) & 1)) {
+                        $this->runLength[0] = -1;
+                        $head = 1;
+                        $this->runLength[$head] = 1;
+                    } elseif ($y > 0) {
+                        if ((ord($frame[$y][$x]) ^ ord($frame[$y - 1][$x])) & 1) {
